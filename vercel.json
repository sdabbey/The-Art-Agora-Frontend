{
  "version": 2,
  "builds": [
    {
      "src": "api/send_email.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/assets/css/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/assets/images/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/assets/audio/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/assets/js/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/send_email",
      "dest": "api/send_email.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
